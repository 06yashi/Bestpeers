<%= form_with(model: @booking, url: bookings_path, local: true) do |form| %>
  <%= form.hidden_field :car_id, value: @car.id %> 
  
  <%= form.label :start_date %>
  <%= form.date_select :start_date %>

  <%= form.label :end_date %>
  <%= form.date_select :end_date %>

  <%= form.submit 'Book Now' %>
<% end %>
