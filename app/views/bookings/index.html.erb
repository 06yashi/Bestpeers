<h1>Your Bookings</h1>

<table>
  <thead>
    <tr>
      <th>Car</th>
      <th>Start Date</th>
      <th>End Date</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    <% @bookings.each do |booking| %>
      <tr>
        <td><%= booking.car.title %></td>
        <td><%= booking.start_date %></td>
        <td><%= booking.end_date %></td>
        <td><%= booking.status || "Pending" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
